{
    "extends": ["plugin:prettier/recommended","eslint:recommended", "plugin:@typescript-eslint/recommended", "plugin:vue/vue3-recommended"],
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "parser": "@typescript-eslint/parser",
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "root": true,
    "rules": {
        "max-len": ["error", { "code": 90 }]
    }
}